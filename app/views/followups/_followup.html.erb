<% if followup %>
  <div>
    <%= button_to resolve_followup_path(followup), method: :patch, class: "main-btn btn-sm success-btn btn-hover", id:"resolve" do %>
      <i class="bi bi-calendar"></i>Resolve Reminder
    <% end %>
  </div>
<% else %>
    <%= render_followup_button(entity) %>
  </div>
<% end %>
